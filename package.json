{
  "name": "ai-react-starter",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.2.0",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "format": "prettier --write src vite.config.ts tsconfig.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test:run": "vitest run",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "validate": "bun run format && bun run typecheck && bun run lint && bun run test:run",
    "verify": "bun run verify:tooling && bun run verify:vite && bun run verify:types && bun run verify:react && bun run verify:tailwind && bun run verify:ui",
    "verify:tooling": "bun scripts/verify-tooling.js",
    "verify:vite": "bun scripts/verify-vite.js",
    "verify:types": "bun scripts/verify-types.js",
    "verify:react": "bun scripts/verify-react.js",
    "verify:tailwind": "bun scripts/verify-tailwind.js",
    "verify:ui": "bun scripts/verify-ui.js"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^28.0.0",
    "prettier": "^3.8.1",
    "shadcn": "^3.8.4",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.0"
  }
}
